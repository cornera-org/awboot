{
    "makefile.extensionOutputFolder": "./.vscode",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**"
    ],
    "cmake.configureOnOpen": true,
    "files.exclude": {
        "**/*.o": true,
    },
    "files.watcherExclude": {
        "**/.git/**": true,
    },
    "cortex-debug.variableUseNaturalFormat": true,
    "files.associations": {
      "condition_variable": "c",
      "cstdint": "c",
      "board.h": "c",
      "optional": "c",
      "ratio": "c",
      "system_error": "c",
      "array": "c",
      "functional": "c",
      "tuple": "c",
      "type_traits": "c",
      "utility": "c",
      "variant": "c",
      "charconv": "c",
      "chrono": "c",
      "format": "c"
    },
}
